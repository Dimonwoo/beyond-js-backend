{
  "scope": "beyond-js",
  "name": "backend",
  "version": "0.1.0",
  "license": "MIT",
  "modules": {
    "path": "./modules"
  },
  "libraries": {
    "imports": [
      "@beyond-js/kernel",
      "@beyond-js/local"
    ]
  },
  "deployment": {
    "distributions": [
      {
        "name": "npm",
        "maps": "external",
        "ports": {
          "bundles": 9104,
          "http": 9105,
          "inspect": 9106
        },
        "npm": {
          "platforms": {
            "web": "web",
            "backend": "backend"
          }
        }
      },
      {
        "name": "web-tsc",
        "platform": "web",
        "ports": {
          "bundles": 9107
        },
        "ts": {
          "compiler": "tsc"
        }
      },
      {
        "name": "backend-tsc",
        "platform": "backend",
        "ports": {
          "bundles": 9108
        },
        "ts": {
          "compiler": "tsc"
        }
      }
    ]
  }
}
